<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
<!-- MoominStand2 -->
    <servlet>
		<servlet-name>MoominStand2</servlet-name>
		<servlet-class>moomin.MoominStand2Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MoominStand2</servlet-name>
		<url-pattern>/moominstand2</url-pattern>
	</servlet-mapping>
<!-- CartServlet -->	
	<servlet>
		<servlet-name>cart</servlet-name>
		<servlet-class>moomin.CartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cart</servlet-name>
		<url-pattern>/cart</url-pattern>
	</servlet-mapping>
<!--  LoginServlet-->	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>moomin.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
<!--  AddLinkDataServlet-->	
	<servlet>
    	<servlet-name>add_linkdata</servlet-name>
    	<servlet-class>moomin.AddLinkDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>add_linkdata</servlet-name>
    	<url-pattern>/add</url-pattern>
	</servlet-mapping>
		<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>